{
    "version": 2,
    "builds": [
      {
        "src": "API/chat.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "^/api/chat$",
        "dest": "/API/chat.js"
      },
      {
        "src": "^/$",
        "dest": "/public/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  