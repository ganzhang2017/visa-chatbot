{
  "functions": {
    "api/chat.js": {
      "maxDuration": 30
    },
    "api/upload.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    }
  ],
  "headers": [
    {
      "source": "/api/index",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}